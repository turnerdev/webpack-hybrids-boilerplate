{
  "name": "webpack-hybrids-boilerplate",
  "version": "1.0.0",
  "description": "Webpack + Hybrids boilerplate project",
  "private": true,
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --reporter spec test/**/*-test.js",
    "watch": "webpack -d --config ./webpack/webpack.config.js --watch --display-error-details",
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --open --display-error-details",
    "build": "webpack -p --config ./webpack/webpack.config.js --display-error-details"
  },
  "author": "Turner",
  "license": "ISC",
  "dependencies": {
    "hybrids": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "eslint": "^5.15.3",
    "eslint-loader": "^2.1.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
